<script>
    if (typeof window.hasAds == "undefined" || window.hasAds != false) {
        $(function () {
            var mediaQuery = typeof Foundation != "undefined" ? Foundation.MediaQuery.current : "";
            googletag.cmd.push(function () {
                var slts = window.slotsToload || [];

                if ((mediaQuery === "small" || mediaQuery == "medium") && window.fontName == "NOTICIA") {
                    slts.push("MrecPrincipalMobile")
                    if ($("#MrecPrincipalAd300x250").length > 0) slts.push("MrecPrincipal300250");
                } else {
                    slts.push("MrecPrincipal")
                    if ($("#MrecPrincipalAd300x250").length > 0) slts.push("MrecPrincipal300250");
                }

                var wasSub = false;
                try {
                    var jp = localStorage.getItem("subscriber_ads");
                    if (jp != null) {
                        try {
                            var jpj = JSON.parse(jp);
                            if (jpj.isSubscriber) {
                                wasSub = true;
                                userOnPage.isSubscriber = true;
                            }
                        } catch (e) { }
                    }
                } catch (e) { }


                var isAtMinute = false;
                if (Publico) {
                    if (window.pageTags && Array.isArray(window.pageTags) && window.pageTags.length > 0) {
                        if (window.pageTags.indexOf("Ao Minuto") > -1 && Publico.Url.param("FromApp") == "1") {
                            isAtMinute = true;
                        }
                    }
                }

                if (!userOnPage.isSubscriber && !wasSub && !isAtMinute) {
                    slts.push("billboardSlot");
                    slts.push("pixelSlot");
                    //$("#billboardAd").closest("aside").show();
                    $("#billboardAd").closest("aside").addClass("ad-slot--loading").show();
                } else {
                    $("#billboardAd").closest("aside").remove();
                }

                window.dfpSlots = [];
                for (i = 0; i < slts.length; i++) {
                    dfpSlots.push(eval(slts[i]));
                }

                googletag.pubads().refresh(dfpSlots);

                googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                    window.adInterval = null;

                    if (typeof userOnPage != "undefined" && "id" in userOnPage) {
                        window.userAds = window.userAds || [];
                        if (!event.isEmpty) {
                            userAds.push({
                                s: event.slot.getName(),
                                advid: event.advertiserId,
                                camid: event.campaignId,
                                creid: event.campaignId,
                                empty: false
                            });
                        } else {
                            userAds.push({
                                s: event.slot.getName(),
                                advid: event.advertiserId,
                                camid: event.campaignId,
                                creid: event.campaignId,
                                empty: true
                            });
                        }

                        if (userAds.length == window.dfpSlots.length) {
                            var adViewByUser = 0;
                            window.userAds.forEach(function (a) { if (!a.empty) { adViewByUser++; } });
                            _paq.push(['setCustomVariable', 3, "adsView", adViewByUser, "page"]);
                            Publico.Track.Event("PUBLICIDADE", "REPORT", JSON.stringify(window.userAds))
                        }
                        //console.log(event);
                    }

                    if (event && event.isEmpty) {
                        try {
                            var $el = $("#" + event.slot.getSlotId().getDomId());
                            $el.closest("aside").remove();
                            return;
                        } catch (e) {

                        }
                    }
                    if (typeof billboardNewsSlot != "undefined" && event.slot == billboardNewsSlot) {
                        setTimeout(function () {
                            try {
                                $("#billboardNewsSlotAd").find("iframe").eq(0).height($("#billboardNewsSlotAd").find("iframe").eq(0).contents().height());
                            } catch (e) {
                                $("#billboardNewsSlotAd").find("iframe").eq(0).height($("#billboardNewsSlotAd").find("iframe").eq(0).height());
                            }

                        }, 500);
                    }
                    if (typeof billboardSlot != "undefined" && event.slot == billboardSlot) {
                        setTimeout(function () {
                            try {
                                $("#billboardAd").find("iframe").eq(0).height($("#billboardAd").find("iframe").eq(0).contents().height());
                            } catch (e) {
                                $("#billboardAd").find("iframe").eq(0).height($("#billboardAd").find("iframe").eq(0).height());
                            }
                            $("#billboardAd").closest("aside").removeClass("ad-slot--loading");

                            if (typeof window.inContent != "undefined") {
                                window.inContentInterval = setInterval(function () {
                                    window.inContent.moveToPosition();
                                }, 500);
                            }

                        }, 500);
                    }
                    if (typeof MrecPrincipal != "undefined" && event.slot == MrecPrincipal) {
                        if (event.size[1] < 500 && (mediaQuery != "small" && mediaQuery != "medium")) {
                            if ($("#ad-slot-main-fallback").length > 0) {
                                $("#ad-slot-main-fallback").show();
                                googletag.pubads().refresh([MrecFallback]);
                                $("#ad-slot-main-fallback").attr("id", "");
                            }
                        }
                        setTimeout(function () {
                            if (mediaQuery === "small" || mediaQuery == "medium") {
                                $("#MrecPrincipalAdMobile").find("iframe").eq(0).height($("#MrecPrincipalAdMobile").find("iframe").eq(0).contents().height());
                            } else {
                                $("#MrecPrincipalAd").find("iframe").eq(0).height($("#MrecPrincipalAd").find("iframe").eq(0).contents().height());
                            }
                        }, 500);
                    }
                    if (typeof pixelSlot != "undefined" && event.slot == pixelSlot) {
                        if (!event.isEmpty) {
                            $("#pixelAd").html($("#pixelAd").find("iframe").contents().find("body").html());
                        }
                    }
                });
            });
        });
    }
</script>

